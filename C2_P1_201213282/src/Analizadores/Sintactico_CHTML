/*codigo publico*/
parser code {:
    public static ArrayList<Nodo_error> Errores = new ArrayList<Nodo_error>();
    public static boolean errores=false;
    protected int error_sync_size () {
       return 1;   
    }

    public void syntax_error(Symbol s){
        System.out.println("Error sintactico lexema \"" + s.value+"\" en la Columna: "+(s.right) +", Linea: "+(s.left+1)+ " no esperado");
        Nodo_error nuevo = new Nodo_error();
        nuevo.dato = String.valueOf(s.value);
        nuevo.linea = (s.left+1);
        nuevo.columna=(s.right);
        Errores.add(nuevo);
        errores=true;
    }

    
    public void unrecovered_syntax_error(Symbol s) throws java.lang.Exception{
        System.out.println("Error sintactico \"" + s.value+"\" en la Linea: "+(s.right+1) +", Columna: "+(s.left+1)+ "");
        Nodo_error nuevo = new Nodo_error();
        nuevo.dato = String.valueOf(s.value);
        nuevo.linea = (s.left+1);
        nuevo.columna=(s.right);
        Errores.add(nuevo);
        errores=true;
    }

 /*codig de usuario */
 String total = "";
 public static Raiz_CHTML=null;
 public ArrayList<Nodo> e = new ArrayList<Nodo>();
:}
action code {:
    public String estado="";
:}
/* terminales del lenguaje*/
terminal IHTML,FHTML,IENCA,ICUER,INI,FIN,FENCA,ICJS,FCJS,ICCS,FCCS,ITIT,FTIT,PRUT,SIGU,SPYC,PCOM,PNOR,FCUER,IEPA,FEPA,IETE,FETE,IEBO,FEBO,IEIM,FEIM,IEEN,FEEN,IETB,FETB,IEFI,FEFI,IECB,FECB,IECT,FECT,IETA,FETA,IECA,FECA,IECJ,FECJ,IEOP,FEOP,ESAL,IESP,FESP,PRUT,PVAL,PID,PGRP,PALT,PANC,PALI,PCLI,PFON;
/*aqui estan los no terminales */
non terminal Start,UNO; 
non terminal NCHTML CUERPO_CHTML,CUERPO_ENCA,CUERPO_CUERPO,ESTRUCTURA_CCSS,ESTRUCTURA_CJS,ESTRUCTURA_TITU,ESTRUCTURA_CUERPO,ESTRUCTURA_G_CUERPO1,ESTRUCTURA_G_CUERPO;
start with Start;
Start ::= UNO;
/*----------------------------ESTRUCTURA DE CHTML-------------------------*/
UNO  ::= INI IHTML FIN CUERPO_CHTML:e1 INI FHTML FIN{:Raiz_CHTML = e1;:};
CUERPO_CHTML ::= INI IENCA FIN CUERPO_ENCA:e1 INI FENCA FIN ESTRUCTURA_CUERPO:e2{:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="CHTML"; nuevo.hijos.add(e1); nuevo.hijos.add(e2); return nuevo;:};
/*----------------------------ESTRUCTURA DE ENCABEZADO-------------------------*/
CUERPO_ENCA ::=     CUERPO_ENCA:e1 ESTRUCTURA_CCSS:e2   {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="encabezado"; NCHTML uno=e1; NCHTML dos=e2; nuevo.hijos.add(uno); nuevo.hijos.add(dos); return nuevo;:}
                |   CUERPO_ENCA:e1 ESTRUCTURA_CJS:e2    {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="encabezado"; NCHTML uno=e1; NCHTML dos=e2; nuevo.hijos.add(uno); nuevo.hijos.add(dos); return nuevo;:}
                |   CUERPO_ENCA:e1 ESTRUCTURA_TITU:e2   {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="encabezado"; NCHTML uno=e1; NCHTML dos=e2; nuevo.hijos.add(uno); nuevo.hijos.add(dos); return nuevo;:}
                |   ESTRUCTURA_TITU:e1                  {:return e1;:}
                |   ESTRUCTURA_CJS:e1                   {:return e1;:}
                |   ESTRUCTURA_CCSS:e1                  {:return e1;:};
/*CCSS*/                
ESTRUCTURA_CCSS::= INI ICCS PRUT SIGU PCOM:e1 SPYC FIN INI FCCS FIN{:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="CCSS"; nuevo.contenido=e1; return nuevo;:};
/*CJS*/
ESTRUCTURA_CJS ::= INI ICJS PRUT SIGU PCOM:e1 SPYC FIN INI FCJS FIN{:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="CJS"; nuevo.contenido=e1; return nuevo;:};
/*titulo*/
ESTRUCTURA_TITU::= INI ITIT FIN  PNOR:e1 INI  FTIT FIN{:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="Titulo"; nuevo.contenido=e1; return nuevo;:};
/*----------------------------ESTRUCTURA DE CUERPO-------------------------*/
ESTRUCTURA_CUERPO ::= INI ICUER                     FIN CUERPO_CUERPO:e2 INI FCUER FIN      {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="Cuerpo"; nuevo.hijos.add(e2); return nuevo;:}
                    | INI ICUER PFON SIGU PCOM:e1 SPYC FIN CUERPO_CUERPO:e2 INI FCUER FIN   {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="Cuerpo"; NCHTML nuevo2 = new NCHTML(); nuevo2.etiqueta="fondo"; nuevo2.contenido=e1;nuevo.atributos.add(e1); nuevo.hijos.add(e2); return nuevo;:};
/*panel*/
ESTRUCTURA_PANEL ::= INI IEPA FIN CONTENIDO_CUERPO:e1 INI FEPA FIN                          {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="Panel"; nuevo.hijos.add(e1); return nuevo;:}
                    |INI IEPA ESTRUCTURA_G_CUERPO:e1  FIN CONTENIDO_CUERPO:e2 INI FEPA FIN  {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="Panel"; nuevo.atributos.add(e1); nuevo.hijos.add(e2); return nuevo;:}; 
/*texto*/
ESTRUCTURA_TEXTO ::= INI IETE FIN  PNOR:e1 INI FETE FIN                         {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="TEXTO"; nuevo.contenido=e1; return nuevo;:}
                    |INI IETE ESTRUCTURA_G_CUERPO:e2 FIN PNOR:e1 INI FETE FIN   {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="TEXTO"; nuevo.contenido=e1; return nuevo;:};
/*imagen*/
ESTRUCTURA_IMAGE ::= INI IEIM FIN PNOR:e1 INI IEFI FIN                      {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="imagen"; nuevo.contenido=e1; return nuevo;:}
                    |INI IEIM ESTRUCTURA_G_SCL:e1 FIN PNOR:e2 INI IEFI FIN  {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="imagen"; nuevo.contenido=e2; nuevo.atributos.add(e2); return nuevo;:}
                    |INI IEIM ESTRUCTURA_G_COM:e1 FIN INI IEFI FIN          {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="imagen"; nuevo.atributos.add(e1); return nuevo;:};
/*boton*/
ESTRUCTURA_BOTON ::= INI IEBO FIN PNOR:e1 INI FEBO FIN                          {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="boton"; nuevo.contenido=e1; return nuevo;:}
                    |INI IEBO ESTRUCTURA_G_CUERPO:e2 FIN PNOR:e1 INI FEBO FIN   {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="boton"; nuevo.contenido=e1; nuevo.atributos.add(e2); return nuevo;:};   
/*enlace*/
ESTRUCTURA_ENLAC ::= INI IEEN ESTRUCTURA_G_SRU:e2 FIN PNOR:e1 INI FEEN FIN    {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="enlace"; nuevo.contenido=e1; nuevo.atributos.add(e2); return nuevo;:};
/*tabla*/
ESTRUCTURA_TABLA ::= INI IETA FIN CUERPO_TABLA:e1 INI FETA FIN                          {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="tabla"; nuevo.hijos.add(e1); return nuevo;:}
                    |INI IETA ESTRUCTURA_G_CUERPO:e2 FIN CUERPO_TABLA:e1 INI FETA FIN   {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="tabla"; nuevo.atributos.add(e2); nuevo.hijos.add(e1); return nuevo;:};

CUERPO_TABLA     ::= INI IEFI FIN CUERPO_FILA:e2 INI FEFI FIN CUERPO_TABLA:e1       {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="fila"; nuevo.hijos.add(e2);nuevo.hijos.add(e1); return nuevo;:}
                    |INI IEFI FIN CUERPO_FILA:e2 INI FEFI FIN                       {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="fila"; nuevo.hijos.add(e2); return nuevo;:};

CUERPO_FILA     ::=  INI IECB FIN CUERPO_CELDA:e1 INI FECB FIN CUERPO_FILA:e2   {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="cb"; nuevo.hijos.add(e1);nuevo.hijos.add(e2); return nuevo;:}
                    |INI IECT FIN CUERPO_CELDA:e1 INI FECT FIN CUERPO_FILA:e2   {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="ct"; nuevo.hijos.add(e1);nuevo.hijos.add(e2); return nuevo;:}
                    |INI IECT FIN CUERPO_CELDA:e1 INI FECT FIN                  {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="ct"; nuevo.hijos.add(e1); return nuevo;:}
                    |INI IECB FIN CUERPO_CELDA:e1 INI FECB FIN                  {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="cb"; nuevo.hijos.add(e1); return nuevo;:};

CUERPO_CELDA  ::=    ESTRUCTURA_BOTON:e1    {:return e1;:}
                    |ESTRUCTURA_TEXTO:e1    {:return e1;:}
                    |ESTRUCTURA_IMAGE:e1    {:return e1;:};
/*text area*/
ESTRUCTURA_TEXTO_A ::= INI IETA ESTRUCTURA_G_CUERPO:e2 FIN PNOR:e1 INI FETA FIN     {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="area"; nuevo.contenido=e1; nuevo.atributos.add(e2); return nuevo;:}
                    |  INI IETA FIN PNOR:e1 INI FETA FIN                            {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="area"; nuevo.contenido=e1; return nuevo;:};
/*text INPUT*/
ESTRUCTURA_CAJA_T  ::=  INI IECA ESTRUCTURA_G_CUERPO:e2 FIN PNO:e1 INI FECA FIN     {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="input"; nuevo.contenido=e1; nuevo.atributos.add(e2); return nuevo;:}
                    |   INI IECA FIN PNO:e1 INI FECA FIN                            {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="input"; nuevo.contenido=e1; return nuevo;:}; 
/*combo box*/
ESTRUCTURA_CAJA_T  ::=  INI IECJ ESTRUCTURA_G_SCL:e2 FIN CUERPO_CAJA:e1 INI FECJ FIN    {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="combobox"; nuevo.hijos.add(e1); nuevo.atributos.add(e2); return nuevo;:}
                    |   INI IECJ FIN CUERPO_CAJA:e1 INI FECJ FIN                        {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="combobox"; nuevo.hijos.add(e1); return nuevo;:};

CUERPO_CAJA        ::=  INI IEOP PVAL SIGU PCOM:e1 SPYC FIN PNO:e2 INI FEOP FIN CUERPO_CAJA:e3 {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta =e1; nuevo.contenido=e2; nuevo.hijos.add(e3); return nuevo;:}
                    |   INI IEOP PVAL SIGU PCOM:e1 SPYC FIN PNO:e2 INI FEOP FIN                {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta =e1; nuevo.contenido=e2; return nuevo;:};
/*estructura spiner*/
ESTRUCTURA_SPINNER ::= INI IESP FIN PNO:e1 INI FESP FIN                           {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="spiner"; nuevo.contenido=e1; return nuevo;:};
                    |  INI IESP ESTRUCTURA_G_CUERPO:e2 FIN PNO:e1 INI FESP FIN    {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="spiner"; nuevo.contenido=e1; nuevo.atributos.add(e2); return nuevo;:};                     
/*SALTO DE LINEA*/
ESTRUCTURA_SALOT   ::= INI ESAL FIN{:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="enter"; return nuevo;:};
/*--------------ESTRUCTURA Generales de CUERPO--------------*/
/*Estrucutura general completa*/ 
ESTRUCTURA_G_COM ::= ESTRUCTURA_G_COM:e2  ESTRUCTURA_G_RUT:e1       {:e2.hijos.add(e1); return e2;:}
                    |ESTRUCTURA_G_COM:e2  ESTRUCTURA_G_CLI:e1       {:e2.hijos.add(e1); return e2;:}
                    |ESTRUCTURA_G_COM:e2  ESTRUCTURA_G_CUERPO:e1    {:e2.hijos.add(e1); return e2;:}
                    |ESTRUCTURA_G_CLI:e1                            {:return e1;:}
                    |ESTRUCTURA_G_CUERPO:e1                         {:return e1;:}
                    |ESTRUCTURA_G_RUT:e1                            {:return e1;:};
/*Estrucutura general con ruta*/                    
ESTRUCTURA_G_SRU ::= ESTRUCTURA_G_SRU:e2 ESTRUCTURA_G_RUT:e1        {:e2.hijos.add(e1); return e2;:}
                    |ESTRUCTURA_G_SRU:e2 ESTRUCTURA_G_CUERPO:e1     {:e2.hijos.add(e1); return e2;:}
                    |ESTRUCTURA_G_CUERPO:e1                         {:return e1;:}
                    |ESTRUCTURA_G_RUT:e1                            {:return e1;:};
/*Estrucutura general con clic*/ 
ESTRUCTURA_G_SCL ::= ESTRUCTURA_G_CLI:e1 ESTRUCTURA_G_SCL:e2        {:e1.hijos.add(e2); return e1;:}
                    |ESTRUCTURA_G_CUERPO:e1 ESTRUCTURA_G_SCL:e2     {:e1.hijos.add(e2); return e1;:}
                    |ESTRUCTURA_G_CLI:e1                            {:return e1;:}
                    |ESTRUCTURA_G_CUERPO:e1                         {:return e1;:};
/*estructura ruta*/                    
ESTRUCTURA_G_RUT ::= PRUT SIGU PCOM:e1 SPYC{:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="ruta"; nuevo.contenido=e1; return nuevo;:};
/*estructura clic*/
ESTRUCTURA_G_CLI ::= PCLI SIGU PCOM:e1 SPYC{:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="clic"; nuevo.contenido=e1; return nuevo;:};
/*estructura varios elementos*/
ESTRUCTURA_G_CUERPO::=  ESTRUCTURA_G_CUERPO1:e2 ESTRUCTURA_G_CUERPO:e1  {:e2.hijos.add(e1); return e2;:} 
                    |   ESTRUCTURA_G_CUERPO1:e2                         {:return e2;:}; 
/*estructura elementos*/                 
ESTRUCTURA_G_CUERPO1::= PGRP SIGU PCOM:e1 SPYC  {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="grupo"; nuevo.contenido=e1; return nuevo;:}
                    |   PID  SIUG PCOM:e1 SPYC  {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="id"; nuevo.contenido=e1; return nuevo;:}
                    |   PALI SIGU PCOM:e1 SPYC  {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="alineado"; nuevo.contenido=e1; return nuevo;:}
                    |   PALT SIGU PCOM:e1 SPYC  {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="alto"; nuevo.contenido=e1; return nuevo;:} 
                    |   PANC SIGU PCOM:e1 SPYC  {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="ancho"; nuevo.contenido=e1; return nuevo;:};                   
