                    |INI IEBO{:System.out.println("entro");:} ESTRUCTURA_G_CUERPO:e1 FIN PNOR:e2 INI FEBO FIN   {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="boton"; nuevo.contenido=e2.toString(); nuevo.atributos.add(e1); RESULT = nuevo;:}   
