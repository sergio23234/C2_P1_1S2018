CUERPO_CUERPO     ::= CUERPO_CUERPO:e1 INI GENERAL_CUERPO:e2{:e1.hijos.add(e2); RESULT= e1;:}
                    | INI GENERAL_CUERPO:e1{:RESULT= e1;:};

GENERAL_CUERPO ::=   ESTRUCTURA_PANEL:e1    {:RESULT=e1;:}
                    |ESTRUCTURA_TEXTO:e1    {:RESULT=e1;:};
/*panel
ESTRUCTURA_PANEL ::= IEPA FIN CUERPO_CUERPO:e1 INI FEPA FIN                          {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="Panel"; nuevo.hijos.add(e1); RESULT = nuevo;:}
                    |IEPA ESTRUCTURA_G_CUERPO:e1  CUERPO_CUERPO:e2 INI FEPA FIN  {:NCHTML nuevo=new NCHTML(); nuevo.etiqueta ="Panel"; nuevo.atributos.add(e1); nuevo.hijos.add(e2); RESULT = nuevo;:}; 

